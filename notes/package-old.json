{
  "name": "chatbot",
  "version": "1.0.0",
  "description": "Chatbot for Meta Business Services",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "devjs": "nodemon ./dist/index.js",
    "compile": "tsc --build --clean && tsc",
    "exec": "node ./dist/index.js",
    "cnrdev": "npm run compile && npm run devjs",
    "cnr": "npm run compile && npm run exec",
    "devts": "nodemon --exec ts-node ./src/index.ts",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts"
  },
  "author": "MarceloSC",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "express": "^4.18.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "typescript": "^5.1.6"
  }
}
